{
  "App": {
    "SelfUrl": "https://localhost:44374"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost; Database=ByteDash.Manpower.LicenseServer; Trusted_Connection=True; TrustServerCertificate=True;"
  },
  "AuthServer": {
    "Authority": "https://localhost:44374",
    "CertificatePassPhrase": "d17d2fd3-cd01-4856-a0d1-e06a5e51eba8"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Secret": "replace_this_with_a_strong_secret_key",
    "Salt": "replace_this_with_a_unique_salt_for_password_hashing",
    "AccessTokenExpirationInDays": 1,
    "RefreshTokenExpirationInDays": 7
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}